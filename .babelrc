{
  "ignore": [
    "node_modules/**"
  ],
  "env": {
    "test": {
      "presets": [
        "flow",
        "env",
        "stage-2"
      ]
    },
    "development": {
      "presets": [
        "flow",
        [
          "env",
          {
            "modules": false
          }
        ],
        "stage-2"
      ],
      "plugins": [
        "external-helpers"
      ]
    },
    "production": {
      "presets": [
        "flow",
        [
          "env",
          {
            "modules": false
          }
        ],
        "stage-2"
      ],
      "plugins": [
        "external-helpers"
      ]
    }
  }
}
